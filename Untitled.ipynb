    "\n",
    "k = 1.0\n",
    "e = 2.7182\n",
    "while(True):\n",
    "    if 2**k / math.factorial(int(k)) < 0.001 * (e ** 2):\n",
    "        break\n",
    "    k += 1.0\n",
    "print(k)"
